{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "functions": {
    "index.js": {
      "maxDuration": 10,
      "runtime": "nodejs18.x"
    }
  },
  "routes": [
    {
      "dest": "index.js",
      "methods": [
        "GET",
        "POST",
        "PUT",
        "PATCH",
        "OPTIONS",
        "DELETE",
        "HEAD",
        "CONNECT",
        "TRACE"
      ],
      "src": "/.*"
    }
  ]
}